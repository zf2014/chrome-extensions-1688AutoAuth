{
  "name": "1688认证自动化插件",
  "version": "3.0.1",
  "manifest_version": 2,
  "description": "1688认证自动化插件",
  "permissions": [
    "storage",
    "tabs",
    "app",
    "https://apiuat.sgsonline.com.cn/ticSend/openapi/api.v1.send/SysMailMsgSendAction/sendTemplateMail"
  ],
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["lib/date_fns.js", "background-script/main.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://agate.1688.com/auth/deep/deep_auth_list.htm"],
      "all_frames": true,
      "js": ["lib/date_fns.js", "lib/jquery.min.js", "content-script/list.js"]
    },

    {
      "matches": ["http://agate.1688.com/auth/deep/deep_auth_invite.htm?*"],
      "all_frames": true,
      "js": [
        "lib/md5.js",
        "lib/date_fns.js",
        "lib/jquery.min.js",
        "content-script/invite.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "page-action/index.html",
    "default_icon": {
      "16": "icon32.png",
      "32": "icon32.png"
    }
  }
}
